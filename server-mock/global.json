{
  "configurationServerURL": "http://localhost:9010",
  "notificationServerURL": "ws://localhost:9000",
  "dynamicConfigurationEnabled": false,
  "preFilters": {
    "domain": {
      "__class": "io.archura.router.config.GlobalConfiguration$FilterConfiguration",
      "parameters": {}
    },
    "tenant": {
      "__class": "io.archura.router.config.GlobalConfiguration$TenantFilterConfiguration",
      "parameters": {},
      "extractConfiguration": {
        "headerConfiguration": {
          "name": "X-Tenant-ID",
          "regex": "(?<tenantId>.*)",
          "captureGroups": [
            "tenantId"
          ]
        },
        "pathConfiguration": {
          "regex": "\\/(?<tenantId>.*)\\/.*",
          "captureGroups": [
            "tenantId"
          ]
        },
        "queryConfiguration": {
          "name": "tenant_id",
          "regex": "(?<tenantId>.*)",
          "captureGroups": [
            "tenantId"
          ]
        }
      }
    },
    "blackList": {
      "__class": "io.archura.router.config.GlobalConfiguration$BlackListFilterConfiguration",
      "parameters": {},
      "ips": [
        "10.20.30.40",
        "50.60.70.80"
      ],
      "domainIps": {
        "localhost:8080": ["10.20.30.40"]
      }
    }
  },
  "domains": {
    "localhost:8080": {
      "name": "localhost:8080",
      "customerAccount": "localhost",
      "defaultTenantId": "default",
      "preFilters": {},
      "postFilters": {},
      "tenants": {
        "default": {
          "name": "default",
          "preFilters": {
            "routeMatching": {
              "__class": "io.archura.router.config.GlobalConfiguration$FilterConfiguration",
              "parameters": {}
            }
          },
          "postFilters": {},
          "methodRoutes": {
            "GET": [
              {
                "name": "route-01",
                "preFilters": {},
                "postFilters": {},
                "matchConfiguration": {
                  "pathConfiguration": {
                    "regex": ".*",
                    "captureGroups": []
                  }
                },
                "extractConfiguration": {},
                "mapConfiguration": {
                  "url": "http://localhost:9020/",
                  "methodMap": {
                    "GET": "GET"
                  },
                  "headers": {}
                }
              }
            ]
          }
        }
      }
    }
  }
}